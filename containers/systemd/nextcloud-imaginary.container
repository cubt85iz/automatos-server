[Unit]
Description=Container service for Nextcloud Imaginary
Requires=network-online.target nss-lookup.target
After=network-online.target nss-lookup.target

[Container]
ContainerName=%p
Image=docker.io/nextcloud/aio-imaginary:latest
EnvironmentFile=/home/core/.config/containers/config/%p.env
AddCapability=SYS_NICE
Tmpfs=/tmp
Network=nextcloud
AutoUpdate=registry
Pull=always

[Service]
Restart=on-failure

[Install]
WantedBy=default.target